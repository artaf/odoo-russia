<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Sequences for sale.order -->
        <record id="seq_type_sale_order_bank" model="ir.sequence.type">
            <field name="name">Sales Order Bank</field>
            <field name="code">sale.order.bank</field>
        </record>

        <record id="seq_type_sale_order_cash" model="ir.sequence.type">
            <field name="name">Sales Order Cash</field>
            <field name="code">sale.order.cash</field>
        </record>

        <record id="seq_sale_order_bank" model="ir.sequence">
            <field name="name">Sales Order Bank</field>
            <field name="code">sale.order.bank</field>
            <field name="prefix">SOB</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_order_cash" model="ir.sequence">
            <field name="name">Sales Order Cash</field>
            <field name="code">sale.order.cash</field>
            <field name="prefix">SOC</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
